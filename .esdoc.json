{
   "source": "./src",
   "destination": "./docs",
   "plugins": [
      {
         "name": "esdoc-standard-plugin",
         "option": {
            "manual": {
               "index": "./Readme.md",
               "files": [
                  "./manual/overview.md",
                  "./manual/documentation.md",
                  "./manual/tooling.md",
                  "./manual/pagination.md",
                  "./manual/refactoring.md"
               ]
            }
         }
      }
   ]
}